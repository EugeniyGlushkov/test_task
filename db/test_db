USE `test`;

DROP TABLE IF EXISTS `notes`;
CREATE TABLE `notes` (
  id INT(11) PRIMARY KEY NOT NULL AUTO_INCREMENT,
  note_name VARCHAR(255) NOT NULL,
  create_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
  is_performed BOOLEAN
)
  ENGINE = InnoDB
  DEFAULT CHARACTER SET = utf8;

